{
  "name": "senscity",
  "homepage": "http://senscity.s3-website.eu-west-2.amazonaws.com/",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://senscity --acl public-read"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.19",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
